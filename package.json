  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:pm2:prod": "pm2 stop inji-service; pm2 delete inji-service; pm2 start --name inji-service npm -- start",
    "prisma:format": "prisma format",
    "prisma:studio:local": "dotenv -e ./.env.local -- prisma studio",
    "prisma:migrate:local": "dotenv -e ./.env.local -- prisma migrate dev --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:generate:local": "dotenv -e ./.env.local -- prisma generate",
    "prisma:generate:production": "dotenv -e ./.env.production -- prisma generate",
    "prisma:migrate": "prisma migrate dev --preview-feature"
  },
